ticketsManager = Page {
	title: $ticketsManagerNLS.title
    tags: "admin"
    feature:tlm_canaccesspage
	
	/* Parameters here */
	
	/* Variables here */
	selectedTicketlogRecorduid = Variable
	selectedActionrecorduid = Variable
	filter = Variable
	selectedReviewrecorduid = Variable
	selectedReviewType = Variable
	
	/* Datasets and records here */
	rootticketlogs = Dataset {
		view:bw_getrootticketlogs
	}
	
	ticketlogHierarchy = HierarchicalData {
		roots-view:bw_getrootticketlogs with filter to filter
		attribute:recorduid
		children-view:bw_getchildticketlogs
		parameter:parentTicketLogRecorduid 
	}
	
	ticketlogDetails = Record {
		view:bw_getticketlog with selectedTicketlogRecorduid to recorduid
	}
	
	ticketActions = Dataset {
		view:bw_getticketactions with selectedTicketlogRecorduid to ticketlogrecorduid
	}
	
	ticketReviewaccounts = Dataset {
		view:bw_getticketreviewsaccounts with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewallocations = Dataset {
		view:bw_getticketreviewsallocations with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewapplications = Dataset {
		view:bw_getticketreviewsapplications with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewgroups = Dataset {
		view:bw_getticketreviewsgroups with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewidentity = Dataset {
		view:bw_getticketreviewsidentity with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewpermissions = Dataset {
		view:bw_getticketreviewspermissions with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewreconciliations = Dataset {
		view:bw_getticketreviewsreconciliations with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewrepository = Dataset {
		view:bw_getticketreviewsrepository with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewrights = Dataset {
		view:bw_getticketreviewrights with selectedActionrecorduid to ticketactionrecorduid
	}

	ticketReviewapplicationmngt = Dataset {
		view:bw_getticketreviewapplicationmngt with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewgroupmngt = Dataset {
		view:bw_getticketreviewgroupmngt with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketRevieworganisationmngt = Dataset {
		view:bw_getticketrevieworganisationmngt with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewpermissionmngt = Dataset {
		view:bw_getticketreviewpermissionmngt with selectedActionrecorduid to ticketactionrecorduid
	}
	ticketReviewrepositorymngt = Dataset {
		view:bw_getticketreviewrepositorymngt with selectedActionrecorduid to ticketactionrecorduid
	}

	ticketRemediation = Dataset {
		view:bw_getticketremediation with selectedReviewrecorduid to ticketreviewrecorduid
	}
	
	deleteticketsTask = TaskRecord(create "bw_deletetickets")
	deletedebugticketsTask = TaskRecord(create "bw_deletedebugtickets")
	
	/* Page content */
	Splitter {
		size:33%
		disposition:Horizontal
		layout:Layout{grab:horizontal True vertical True}
		
		Group {
			border:True
			Edit {
				variable:filter
			}
			Tree {
				data:ticketlogHierarchy
				layout:Layout{grab:horizontal True vertical True}
				configurable:True
				persist-configuration:True
				
				Column {
					column:recorduid
					hidden:True
					selection:selectedTicketlogRecorduid
				}
				Column {
					column:title
					width:100%
					header:$bw_ticketlog.title
				}
				Column {
					column:submissiondate
					header:$bw_ticketlog.submissiondate
					initially-masked:True
				}
				Column {
					column:duedate
					header:$bw_ticketlog.duedate
					initially-masked:True
				}
				Column {
					column:closedate
					header:$bw_ticketlog.closedate
					initially-masked:True
				}
				Column {
					column:ticketopen
					header:$bw_ticketlog.ticketopen
					initially-masked:True
				}
				Column {
					column:issuerfullname
					header:$bw_ticketlog.issuerfullname
					initially-masked:True
				}
			}		

			ButtonGroup {
				Button {
					actions:Confirm $ticketsManagerNLS.ticketdeleteconfirm1, Confirm $ticketsManagerNLS.ticketdeleteconfirm2, Set selectedTicketlogRecorduid to deleteticketsTask.ticketlog, TaskComplete deleteticketsTask wait, Flash Information $ticketsManagerNLS.ticketdeleteflash, ReloadData (ticketlogHierarchy)
					text:$ticketsManagerNLS.ticketdeletebutton
				    feature:tlm_canpurgetickets
					disabled:OrPredicate {
						BooleanPredicate (ticketlogDetails.ticketopen)
						StringPredicate(selectedTicketlogRecorduid) { when IsEmpty then True }
						StringPredicate(ticketlogDetails.tickettype) {when "PURGELOG-PURGE" then True}
					}
				}
				Button {
					actions:Confirm $ticketsManagerNLS.ticketdeletedebugconfirm1, Confirm $ticketsManagerNLS.ticketdeletedebugconfirm2, Set selectedTicketlogRecorduid to deleteticketsTask.ticketlog, TaskComplete deletedebugticketsTask wait, Flash Information $ticketsManagerNLS.ticketdeletedebugflash, ReloadData (ticketlogHierarchy)
					text:$ticketsManagerNLS.ticketdeletedebugbutton
				    feature:tlm_canpurgedebugtickets
				}
			}			
		}
		Group {
			border:True
			hidden:StringPredicate(selectedTicketlogRecorduid) { when IsEmpty then True }
			TabFolder {
				TabItem {
					text:$ticketsManagerNLS.details
				
					Flow {
						disposition:Horizontal
						Group {
							Text {compact:True label:$bw_ticketlog.title value:ticketlogDetails.title styling:bigTitleStyle}
							Text {compact:True label:$bw_ticketlog.description value:ticketlogDetails.description lines:4}
							Text {compact:True label:$bw_ticketlog.ticketnumber value:ticketlogDetails.ticketnumber}
							Text {compact:True label:$bw_ticketlog.ticketopen value:Transform ticketlogDetails.ticketopen using booleanDisplayMapping}
							Text {compact:True label:$bw_ticketlog.status value:ticketlogDetails.status}
							Text {compact:True label:$bw_ticketlog.^priority value:ticketlogDetails.^priority lines:4}
							Text {compact:True label:$bw_ticketlog.tickettype value:ticketlogDetails.tickettype}
							Text {compact:True label:$bw_ticketlog.duedate value:ticketlogDetails.duedate}
							Text {compact:True label:$bw_ticketlog.closedate value:ticketlogDetails.closedate}
							Text {compact:True label:$bw_ticketlog.issuerfullname value:ticketlogDetails.issuerfullname}
							Text {compact:True label:$bw_ticketlog.externalreference value:ticketlogDetails.externalreference}
							Text {compact:True label:$bw_ticketlog.debugmode value:Transform ticketlogDetails.debugmode using booleanDisplayMapping}
						}
						Group {
							Text {compact:True label:$bw_ticketlog.custom1 value:ticketlogDetails.custom1}
							Text {compact:True label:$bw_ticketlog.custom2 value:ticketlogDetails.custom2}
							Text {compact:True label:$bw_ticketlog.custom3 value:ticketlogDetails.custom3}
							Text {compact:True label:$bw_ticketlog.custom4 value:ticketlogDetails.custom4}
							Text {compact:True label:$bw_ticketlog.custom5 value:ticketlogDetails.custom5}
							Text {compact:True label:$bw_ticketlog.custom6 value:ticketlogDetails.custom6}
							Text {compact:True label:$bw_ticketlog.custom7 value:ticketlogDetails.custom7}
							Text {compact:True label:$bw_ticketlog.custom8 value:ticketlogDetails.custom8}
							Text {compact:True label:$bw_ticketlog.custom9 value:ticketlogDetails.custom9}
						}
						Group {
							Text {compact:True label:$bw_ticketlog.custom10 value:ticketlogDetails.custom10}
							Text {compact:True label:$bw_ticketlog.custom11 value:ticketlogDetails.custom11}
							Text {compact:True label:$bw_ticketlog.custom12 value:ticketlogDetails.custom12}
							Text {compact:True label:$bw_ticketlog.custom13 value:ticketlogDetails.custom13}
							Text {compact:True label:$bw_ticketlog.custom14 value:ticketlogDetails.custom14}
							Text {compact:True label:$bw_ticketlog.custom15 value:ticketlogDetails.custom15}
							Text {compact:True label:$bw_ticketlog.custom16 value:ticketlogDetails.custom16}
							Text {compact:True label:$bw_ticketlog.custom17 value:ticketlogDetails.custom17}
							Text {compact:True label:$bw_ticketlog.custom18 value:ticketlogDetails.custom18}
							Text {compact:True label:$bw_ticketlog.custom19 value:ticketlogDetails.custom19}				
						}
					}
				}
				TabItem {
					text:$ticketsManagerNLS.actions
					Table {
						data:ticketActions
						show-count:True
						show-filter:True
						persist-configuration:True
						configurable:True
						label:$ticketsManagerNLS.actions
						layout:Layout  {grab:horizontal True vertical True}

						Column {column: recorduid header:$bw_ticketaction.recorduid hidden:True selection:selectedActionrecorduid}
						Column {column: taskid header:$bw_ticketaction.taskid sortable:True filterable:True initially-masked:True}
						Column {column: startdate header:$bw_ticketaction.startdate  sortable:True filterable:True width:130px}
						Column {column: enddate header:$bw_ticketaction.enddate  sortable:True filterable:True width:130px}
						Column {column: status header:$bw_ticketaction.status  sortable:True filterable:True}
						Column {column: title header:$bw_ticketaction.title  sortable:True filterable:True width:240px}
						Column {column: description header:$bw_ticketaction.description  sortable:True filterable:True width:240px}
						Column {column: actorfullname header:$bw_ticketaction.actorfullname  sortable:True filterable:True}
						Column {column: custom1 header:$bw_ticketaction.custom1  sortable:True filterable:True initially-masked:True}
						Column {column: custom2 header:$bw_ticketaction.custom2  sortable:True filterable:True initially-masked:True}
						Column {column: custom3 header:$bw_ticketaction.custom3  sortable:True filterable:True initially-masked:True}
						Column {column: custom4 header:$bw_ticketaction.custom4  sortable:True filterable:True initially-masked:True}
						Column {column: custom5 header:$bw_ticketaction.custom5  sortable:True filterable:True initially-masked:True}
						Column {column: custom6 header:$bw_ticketaction.custom6  sortable:True filterable:True initially-masked:True}
						Column {column: custom7 header:$bw_ticketaction.custom7  sortable:True filterable:True initially-masked:True}
						Column {column: custom8 header:$bw_ticketaction.custom8  sortable:True filterable:True initially-masked:True}
						Column {column: custom9 header:$bw_ticketaction.custom9  sortable:True filterable:True initially-masked:True}
						Column {column: custom10 header:$bw_ticketaction.custom10  sortable:True filterable:True initially-masked:True}
						Column {column: custom11 header:$bw_ticketaction.custom11  sortable:True filterable:True initially-masked:True}
						Column {column: custom12 header:$bw_ticketaction.custom12  sortable:True filterable:True initially-masked:True}
						Column {column: custom13 header:$bw_ticketaction.custom13  sortable:True filterable:True initially-masked:True}
						Column {column: custom14 header:$bw_ticketaction.custom14  sortable:True filterable:True initially-masked:True}
						Column {column: custom15 header:$bw_ticketaction.custom15  sortable:True filterable:True initially-masked:True}
						Column {column: custom16 header:$bw_ticketaction.custom16  sortable:True filterable:True initially-masked:True}
						Column {column: custom17 header:$bw_ticketaction.custom17  sortable:True filterable:True initially-masked:True}
						Column {column: custom18 header:$bw_ticketaction.custom18  sortable:True filterable:True initially-masked:True}
						Column {column: custom19 header:$bw_ticketaction.custom19  sortable:True filterable:True initially-masked:True}
					}
					ConditionalGroup {
						StringCondition(selectedActionrecorduid) {
						when IsEmpty then [
							Header {value:$ticketsManagerNLS.actioninfo}
						]
						otherwise [
							Combo {
								label:$ticketsManagerNLS.rttype
								allow-none:False
								variable:selectedReviewType
								options: Static {
									Option { text:$ticketsManagerNLS.rvaccount value: "account"}
									Option { text:$ticketsManagerNLS.rvallocation value: "allocation"}
									Option { text:$ticketsManagerNLS.rvapplication value: "application"}
									Option { text:$ticketsManagerNLS.rvgroup value: "group"}
									Option { text:$ticketsManagerNLS.rvidentity value: "identity"}
									Option { text:$ticketsManagerNLS.rvpermission value: "permission"}
									Option { text:$ticketsManagerNLS.rvreconciliation value: "reconciliation"}
									Option { text:$ticketsManagerNLS.rvrepository value: "repository"}
									Option { text:$ticketsManagerNLS.rvright value: "right"}

									Option { text:$ticketsManagerNLS.rvappmngt value: "application management"}
									Option { text:$ticketsManagerNLS.rvgroupmngt value: "group management"}
									Option { text:$ticketsManagerNLS.rvorgmngt value: "organisation management"}
									Option { text:$ticketsManagerNLS.rvpermmngt value: "permission management"}
									Option { text:$ticketsManagerNLS.rvpermmngt value: "repository management"}
								}
							}
							ConditionalGroup {
								StringCondition(selectedReviewType) {
									when "account" then [
										Table {
											data:ticketReviewaccounts
											show-count:True
											show-filter:True
											persist-configuration:True
											configurable:True
											label:$ticketsManagerNLS.reviews
											layout:Layout  {grab:horizontal True vertical True}
											
											Column {column:recorduid hidden:True selection:selectedReviewrecorduid}
											Column {column: status header:$bw_ticketreview.status  sortable:True filterable:True}
											Column {column: comment header:$bw_ticketreview.comment  sortable:True filterable:True}
											Column {column: custom1 header:$bw_ticketreview.custom1  sortable:True filterable:True initially-masked:True}
											Column {column: custom2 header:$bw_ticketreview.custom2  sortable:True filterable:True initially-masked:True}
											Column {column: custom3 header:$bw_ticketreview.custom3  sortable:True filterable:True initially-masked:True}
											Column {column: custom4 header:$bw_ticketreview.custom4  sortable:True filterable:True initially-masked:True}
											Column {column: custom5 header:$bw_ticketreview.custom5  sortable:True filterable:True initially-masked:True}
											Column {column: custom6 header:$bw_ticketreview.custom6  sortable:True filterable:True initially-masked:True}
											Column {column: custom7 header:$bw_ticketreview.custom7  sortable:True filterable:True initially-masked:True}
											Column {column: custom8 header:$bw_ticketreview.custom8  sortable:True filterable:True initially-masked:True}
											Column {column: custom9 header:$bw_ticketreview.custom9  sortable:True filterable:True initially-masked:True}
											Column {column: custom10 header:$bw_ticketreview.custom10  sortable:True filterable:True initially-masked:True}
											Column {column: custom11 header:$bw_ticketreview.custom11  sortable:True filterable:True initially-masked:True}
											Column {column: custom12 header:$bw_ticketreview.custom12  sortable:True filterable:True initially-masked:True}
											Column {column: custom13 header:$bw_ticketreview.custom13  sortable:True filterable:True initially-masked:True}
											Column {column: custom14 header:$bw_ticketreview.custom14  sortable:True filterable:True initially-masked:True}
											Column {column: custom15 header:$bw_ticketreview.custom15  sortable:True filterable:True initially-masked:True}
											Column {column: custom16 header:$bw_ticketreview.custom16  sortable:True filterable:True initially-masked:True}
											Column {column: custom17 header:$bw_ticketreview.custom17  sortable:True filterable:True initially-masked:True}
											Column {column: custom18 header:$bw_ticketreview.custom18  sortable:True filterable:True initially-masked:True}
											Column {column: custom19 header:$bw_ticketreview.custom19  sortable:True filterable:True initially-masked:True}

											Column {column: accountlogin header:$paccount.login  sortable:True filterable:True}
											Column {column: accountusername header:$paccount.username sortable:True filterable:True}
											Column {column: repositorycode header:$prepository.code  sortable:True filterable:True initially-masked:True}
											Column {column: repositorydisplayname header:$prepository.displayname  sortable:True filterable:True}
										}
									]								
									when "allocation" then [
										Table {
											data:ticketReviewallocations
											show-count:True
											show-filter:True
											persist-configuration:True
											configurable:True
											label:$ticketsManagerNLS.reviews
											layout:Layout  {grab:horizontal True vertical True}
											
											Column {column:recorduid hidden:True selection:selectedReviewrecorduid}
											Column {column: status header:$bw_ticketreview.status  sortable:True filterable:True}
											Column {column: comment header:$bw_ticketreview.comment  sortable:True filterable:True}
											Column {column: custom1 header:$bw_ticketreview.custom1  sortable:True filterable:True initially-masked:True}
											Column {column: custom2 header:$bw_ticketreview.custom2  sortable:True filterable:True initially-masked:True}
