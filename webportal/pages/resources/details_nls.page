accountDetails = NLS {
	title [en "Account Details" fr "Détails du Compte" ]
	dynamictitle [en "Account Details {0}" fr "Détails du Compte {0}" ]
	subtitle [en "{0,choice,0#Present in the Repository {1} |1#does not have an associated Repository}" fr "{0,choice,0#Présent dans le Référentiel {1} |1#n''a pas de Référenciel associé}"]	
	number_applications [ en "Applications" fr "Applications"]
