controls_SoDtopControlsFltOrg = Report {
	report: "/reports/controls/SoDtopControlsFltOrg.rptdesign" with ^threshold to ^threshold,  recorduidMatch.shortname to ^orgs, ^orgs to ^orgs
	feature: controls_SoDtopControlsFltOrg
	tags:"organisation" priority 400, "byuid", "analytics"
	icon: "16/details/analytics_16.png" large-icon: "48/details/analytics_48.png"  
	title: $report.controls_SoDtopControlsFltOrg.title
	dynamic-title: $report.controls_SoDtopControlsFltOrg.dynamictitle ( organisation.displayname )
	batched-only: True
	
	^threshold = Parameter {
		mandatory: False // Set to false to avoid pop-up when receiving recorduid
		hidden: False
		type: Integer
		default: "80"
		label: $report.controls_SoDtopControlsFltOrg.^threshold
	}
	^orgs = Parameter {
		mandatory: False
		hidden: False
		type: String
		multivalued: True
		label: $report.controls_SoDtopControlsFltOrg.^orgs
		constraint: Lookup {
			data: br_organisation
			value: Current shortname
			text: Current displayname	
		}
	}
	// Added for compliance with new standard
	^recorduid = Parameter {
		mandatory: False
		hidden: True
		type: Integer
		default:-1
		recorduid-of: Organisation
	}
	
	br_organisation = Dataset {
		view: br_organisation
	}
	
	//Added to transform the recorduid to shortname
	recorduidMatch = Record {
		view: br_organisationDetail_uid  with ^recorduid to recorduid
	}
	
	organisation = Record { view: br_organisation with ^recorduid to recorduid }
}

