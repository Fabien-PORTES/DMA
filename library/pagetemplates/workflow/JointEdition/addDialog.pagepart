<template:jointeditionadddialog/> = Dialog {
	title: <template:jointeditionadddialogtitle/>
	mixins: workflowDefaultMixin
	width: 800
	height: <template:jointeditiondialogheight/>
	
	buttons: [
		OkButton {
			disabled: <template:jointeditiondialogcompletecondition/>
		}
		CancelButton
	]
	
	<template:jointeditiondialogvariables/>
	<template:jointeditiondialogdatasets/>	
	<template:jointeditiondialogeditcomponents/>
}
