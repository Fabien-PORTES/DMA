MultiEdit {
	label: <template:variablelabel/>
	add-dialog: repositoryPicker(<template:taskvariablename/> to inExcludedUidList) returning outUid
	Element {
		variable: <template:taskvariablename/>
		filterable: True
		display: Display {
			data: <template:variabledataset/> match uid
			value: Current displayname
		}
	}
}
